{
  "name": "revolucion-de-los-aplausos",
  "version": "1.3.0",
  "scripts": {
    "ng": "ng",
    "start:dev": "ng serve",
    "build:gh": "ng build --configuration production --output-path docs --base-href /revolucion-de-los-aplausos/ && cp docs/index.html docs/404.html",
    "build:prod": "ng build --configuration production",
    "build:docker": "docker build --rm -f Dockerfile -t revolucion-de-los-aplausos:latest .",
    "start:docker": "docker run --rm -d -p 80:80/tcp revolucion-de-los-aplausos:latest",
    "test": "ng test",
    "e2e": "ng e2e",
    "release": "standard-version",
    "commit": "cz",
    "dev:ssr": "ng run revolucion-de-los-aplausos:serve-ssr",
    "serve:ssr": "node dist/revolucion-de-los-aplausos/server/main.js",
    "build:ssr": "ng build -c production && ng run revolucion-de-los-aplausos:server",
    "prerender": "ng run revolucion-de-los-aplausos:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.0.2",
    "@angular/cdk": "^13.0.2",
    "@angular/common": "~13.0.2",
    "@angular/compiler": "~13.0.2",
    "@angular/core": "~13.0.2",
    "@angular/forms": "~13.0.2",
    "@angular/material": "^13.0.2",
    "@angular/platform-browser": "~13.0.2",
    "@angular/platform-browser-dynamic": "~13.0.2",
    "@angular/platform-server": "~13.0.2",
    "@angular/router": "~13.0.2",
    "@nguniversal/express-engine": "^13.0.1",
    "compression": "^1.7.4",
    "express": "^4.15.2",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.0.3",
    "@angular/cli": "~13.0.3",
    "@angular/compiler-cli": "~13.0.2",
    "@nguniversal/builders": "^13.0.1",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "standard-version": "^9.3.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.4.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
